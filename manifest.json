{
    "manifest_version": 3,
    "name": "DartPad Gist Publisher",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://dartpad.dev/*"],
        "js": ["content.js"]
      }
    ]
  }